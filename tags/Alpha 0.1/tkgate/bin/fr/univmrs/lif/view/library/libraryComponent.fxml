<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.VBox?>

<fx:root fx:id="component" alignment="TOP_CENTER" fillWidth="false" type="VBox" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Separator prefWidth="200.0" />
      <Label fx:id="componentName" alignment="CENTER" contentDisplay="CENTER" prefHeight="27.0" prefWidth="200.0" text="Component name" VBox.vgrow="ALWAYS">
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </Label>
      <Separator opacity="0.41" prefWidth="200.0" />
   </children>
</fx:root>
